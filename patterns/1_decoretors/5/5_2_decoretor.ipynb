   "outputs": [],
   "source": [
    "def my_decorator(func):\n",
    "\n",
    "    def wrapper():\n",
    "        print(\"Antes\")\n",
    "        func()\n",
    "        print(\"Depois\")\n",
    "\n",
    "    return wrapper\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "8406e935",
   "metadata": {},
   "outputs": [
