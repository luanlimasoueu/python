     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Antes\n",
      "Oi\n",
      "Depois\n"
     ]
    }
   ],
   "source": [
    "@my_decorator\n",
    "def say_hi():\n",
    "    print(\"Oi\")\n",
    "\n",
    "say_hi()"
   ]
  }
